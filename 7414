GameName = "UTG"

tab1name = "�𧑐�糼BOSS"
tab2name = "銝���BOSS"
tab3name = "�𧑐��BOSS"

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local GameName = Library:NewWindow(GameName)

local Tab1 = GameName:NewSection(tab1name)

Tab1:CreateButton("boss 1", function(value)
local args = {
    [1] = {
        ["hb"] = {
            [1] = workspace.DungeonOfDespair:FindFirstChild("1").Enemies.RayAlpha
        },
        ["action"] = "hit",
        ["combo"] = 1,
        ["c"] = game:GetService("Players").LocalPlayer.Character,
        ["damage"] = 2000000000000000
    }
}

game:GetService("ReplicatedStorage").Remotes.Axe:FireServer(unpack(args))
end)

